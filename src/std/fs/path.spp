use std::boolean::Bool
use std::clone::Clone
use std::iterator::Iterator
use std::iterator::IterRef
use std::ops::eq::Eq
use std::ops::div::Div
use std::ops::ne::Ne
use std::option::None
use std::option::Opt
use std::option::Some
use std::string::Str


cls Path {
    path: Str
}


sup Path {
    @no_impl
    fun is_absolute(&self) -> Bool { }

    @no_impl
    fun is_relative(&self) -> Bool { }

    @no_impl
    fun to_absolute(&self) -> Path { }

    @no_impl
    fun to_relative(&self) -> Path { }

    @no_impl
    fun exists(&self) -> Bool { }

    @no_impl
    fun parent(&self) -> Path { }
}


sup Path {
    @no_impl
    fun startswith(&self, prefix: &Str) -> Bool { }

    @no_impl
    fun endswith(&self, suffix: &Str) -> Bool { }
}


sup Path {
