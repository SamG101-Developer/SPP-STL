use std::allocator::Alloc
use std::allocator::GlobalAlloc
use std::cast::From
use std::clone::Clone
use std::mem::memory::Memory
use std::number::USize
use std::result::Fail
use std::void::Void


@public
cls Arr[T, cmp n: USize] { }


sup [T, cmp n: USize] Arr[T, n] ext Memory[T] {
    fun len(&self) -> USize {
        ret n
    }
}


sup [T, cmp n: USize] Arr[T, n] ext From[Slice[T]] {
    fun from(that: Slice[T]) -> Arr[T, n] {
        ret Arr(ptr=that.ptr, cap=that.cap)
    }
}
