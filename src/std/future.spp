use std::number::U8
use std::option::Opt
use std::threading::atomic::Atom
use std::void::Void


@public
cls Fut[T] {
    @private state: Atom[U8]
    @private val: Opt[T]
}


sup [T] Fut[T] {
    @public
    @compiler_builtin
    fun await(self) -> T { }

    @public
    @compiler_builtin
    fun await_all(..futures: Fut[T]) -> Void { }
}
