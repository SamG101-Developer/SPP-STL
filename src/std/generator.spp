use std::option::None
use std::void::Void


@public
use GenResult[T] = T or None


@public
cls Gen[Yield, Send=Void] { }


@public
cls GenOnce[Yield] { }


sup [Yield, Send] Gen[Yield, Send] {
    @compiler_builtin
    cor resume(&self, value: Send) -> Gen[GenResult[Yield]] { }
}


sup [Yield] GenOnce[Yield] {
    @compiler_builtin
    cor resume(self) -> Gen[GenResult[Yield]] { }
}
