use std::option::None
use std::void::Void


@public
cls Gen[Yield, Send=Void] { }


@public
cls GenOnce[Yield] { }


sup [Yield, Send] Gen[Yield, Send] {
    @virtual_method
    @compiler_builtin
    cor resume(&self, value: Send) -> Gen[Yield or None] { }
