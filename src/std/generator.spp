use std::option::None
use std::option::Opt
use std::void::Void


@public
cls Gen[Yield, Send=Void] { }


@public
cls GenOnce[Yield] { }


sup [Yield, Send] Gen[Yield, Send] {
    @virtual_method
    @compiler_builtin
    cor resume(&mut self, value: Send) -> Opt[Gen[Yield]] { }
}


sup [Yield] GenOnce[Yield] ext Gen[Yield, Void] {
    @compiler_builtin
    cor resume(&mut self, value: Void) -> Opt[Gen[Yield]] { }
}
