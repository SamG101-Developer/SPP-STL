use std::copy::Copy


# The base numeric types in the standard library.
!protected
cls Num { }


# The base integer type that is numeric.
!protected
cls Integer { }


# The base floating point type that is numeric.
!protected
cls FloatingPoint { }


# All numeric types are copyable.
sup Num ext Copy { }


# The base integer type extends the base numeric type.
sup Integer ext Num { }


# The base floating point type extends the base numeric type.
sup FloatingPoint ext Num { }
