use std::option::Opt
use std::option::Some


cls From[This, That] { }


sup [This, That] From[This, That] {
    @public
    @abstract_method
    fun from(that: That) -> This { }

    @public
    @virtual_method
    fun try_from(that: That) -> Opt[This] {
        ret Some(val=From[This, That]::from(that))  # todo: Self::from(that) should work
    }
}
