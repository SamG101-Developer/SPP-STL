use std::generator::GenOnce


@public cls DerefRef[T] { }
@public cls DerefMut[T] { }


sup [T] DerefRef[T] {
    @public
    @abstract_method
    cor deref_ref(&self) -> GenOnce[&T] { }
}


sup [T] DerefMut[T] {
    @public
    @abstract_method
    cor deref_mut(&mut self) -> GenOnce[&mut T] { }
}
