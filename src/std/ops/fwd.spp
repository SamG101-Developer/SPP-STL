use std::generator::GenOnce


!public cls FwdMov[T] { }
!public cls FwdMut[T] { }
!public cls FwdRef[T] { }


sup [T] FwdMov[T] {
    !public
    !abstract_method
    cor fwd_mov(self) -> GenOnce[T] { }
}


sup [T] FwdMut[T] {
    !public
    !abstract_method
    cor fwd_mut(&mut self) -> GenOnce[&mut T] { }
}


sup [T] FwdRef[T] {
    !public
    !abstract_method
    cor fwd_ref(&self) -> GenOnce[&T] { }
}
