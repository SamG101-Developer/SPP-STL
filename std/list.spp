cls SingleLinkedList[T] {
    head: Opt[SingleLinkedNode[T]]
}


cls SingleLinkedNode[T] {
    value: T
    next: Opt[SingleLinkedNode[T]]
}


cls DoubleLinkedList[T] {
    head: Opt[DoubleLinkedNode[T]]
    tail: Opt[DoubleLinkedNode[T]]
}


cls DoubleLinkedNode[T] {
    value: T
    next: Opt[DoubleLinkedNode[T]]
    prev: Opt[DoubleLinkedNode[T]]
}
